<div class="modal" id="record-exist-modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">@ViewBag.RecordExistsModalTitle</h4>
            </div>
            <div class="modal-body  text-center">
                <a data-toggle="modal" data-target="#record-research-first" href="@ViewBag.RecordExistsModalLinkYes" class="btn btn-success">Yes</a>
                <a href="@ViewBag.RecordExistsModalLinkNo" class="btn btn-danger">No</a>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
<div class="modal" id="record-research-first">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">@ViewBag.RecordSearchModalTitle</h4>
            </div>
            <div class="modal-body  text-center">
                <a href="@ViewBag.RecordSearchModalLink" class="btn btn-success">OK</a>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
